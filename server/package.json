{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "description": "The server end of this app.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "scripts": {
    "build": "tsc",
    "debug": "node -r ts-node/register --inspect-brk src/main/index.ts",
    "dist": "node dist",
    "start": "npm run build && npm run dist",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register",
    "update": "npm up --dev"
  },
  "keywords": [
    "server",
    "nodejs",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "@iinfinity/logger": "0.x",
    "@iinfinity/rester": "0.x"
  },
  "devDependencies": {
    "@types/jasmine": "3.x",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/node": "11.x",
    "jasmine": "3.x",
    "jasmine-console-reporter": "3.x",
    "ts-node": "8.x",
    "typescript": "3.x"
  }
}
